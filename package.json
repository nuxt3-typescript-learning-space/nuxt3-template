{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev -o",
    "test": "vitest",
    "test:ui": "vitest --ui --coverage",
    "test:coverage": "vitest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:rules": "npx @eslint/config-inspector@latest",
    "format": "prettier --write .",
    "lint-and-format": "pnpm run lint:fix && pnpm run format",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "convert": "npx vite-node settings/data/convert.ts",
    "predev": "pnpm run convert",
    "prepare": "husky"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.5.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.379.0",
    "nuxt": "^3.12.3",
    "pinia": "^2.1.7",
    "radix-vue": "^1.9.0",
    "shadcn-nuxt": "^0.10.4",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "vue": "^3.4.31",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@nuxt/eslint": "^0.3.13",
    "@nuxt/test-utils": "^3.13.1",
    "@nuxtjs/tailwindcss": "^6.12.0",
    "@types/eslint": "^8.56.10",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/estree": "^1.0.5",
    "@types/node": "^20.14.10",
    "@vanilla-extract/css": "^1.15.3",
    "@vanilla-extract/vite-plugin": "^4.0.13",
    "@vitest/coverage-v8": "^2.0.2",
    "@vitest/ui": "^1.6.0",
    "@vue/test-utils": "^2.4.6",
    "commitizen": "^4.3.0",
    "commitlint-config-gitmoji": "^2.3.1",
    "cz-customizable": "^7.0.0",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "glob": "^10.4.3",
    "globals": "^15.8.0",
    "happy-dom": "^14.12.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "playwright-core": "^1.45.1",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "typescript": "^5.5.3",
    "vite-node": "^1.6.0",
    "vite-plugin-eslint2": "^4.4.0",
    "vitest": "^2.0.2",
    "vue-tsc": "^2.0.26"
  },
  "lint-staged": {
    "**/*.{js,ts,vue}": [
      "pnpm convert",
      "npx eslint --fix"
    ],
    "**/*.{js,ts,vue,json}": "npx prettier --write",
    "**/*.{ts,vue}": "npx nuxi typecheck ."
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cts"
    }
  },
  "volta": {
    "node": "20.15.0",
    "pnpm": "9.7.0"
  }
}
